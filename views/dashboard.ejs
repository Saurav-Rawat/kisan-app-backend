<h1>secfret page</h1>
<% let id = "5d5d13bd9972343e54e72f13"%>
<%= typeof message == 'undefined'? ' ': message %>
<form action="/match?id=<%=id %>" method="POST" enctype="multipart/form-data">
    <div class="file-field input-field">
      <div class="btn grey">
        <span>File</span>
        <input name="player" type="file">
      </div>
      <div class="file-path-wrapper">
        <input class="file-path validate" type="text">
      </div>
    </div>
    <input name="entryFee" type="number">
    <button type="submit" class="btn">Submit</button>
  </form><br><br><br>
<a href="/logout">logout</a>